---
layout: main.layout
titleSeparator: '-'
titleName: 'ModularAdmin | HTML version'
---

<!--
  You may use the following felper classes on the .Dashboard element

  -dafault                    Default layout options

  -header-fixed               Fixed header on all viewports
  -header-fixed-mobile        Fixed header on all viewports
  -sidebar-compact-tablet     Compact sidebar on tablet viewports
  -sidebar-open-tablet        Open sidebar on tabler viewports
  -sidebar-compact-desktop    Compact sidebar on desktop viewports
  -customize-open             Open customize menu panel
-->
<div
  id="Dashboard"
  class="
    Dashboard
  " >

  <div class="HeaderContainer">
    {{> Dashboard/_common/DashboardHeader/DashboardHeader}}
  </div>
  <div class="SidebarContainer">
    {{> Dashboard/_common/DashboardSidebar/DashboardSidebar}}
  </div>
  <div class="SidebarOverlay" id="SidebarOverlay"></div>
  <div class="ContentContainer {{pagename}}">
    {{!-- <div class="ContentBackground"></div> --}}
    <div class="Content">
      {{{body}}}
    </div>
  </div>

  <div class="FooterContainer" >
    {{> Dashboard/_common/DashboardFooter/DashboardFooter}}
  </div>

  {{> Dashboard/_common/DashboardSearch/DashboardSearch}}

  <div class="CustomizeContainer">
    {{> Dashboard/_common/DashboardCustomize/DashboardCustomize}}
  </div>

</div>
